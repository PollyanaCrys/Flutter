class InfoCatalogo {
  final String imagem;
  final String titulo;
  final String descricao;
  final String quantidade;
  final int valor;
    
  InfoCatalogo({
    required this.imagem,
    required this.titulo,
    required this.descricao,
    required this.quantidade,
    required this.valor,
      });
}
   
